<keymap version="1" name="IntelliJ IDEA Classic copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl r" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl r" />
  </action>
  <action id="Replace" />
  <action id="SavaAs" />
  <action id="linessorter.sort">
    <keyboard-shortcut first-keystroke="shift alt l" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
</keymap>