<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bdebug\b.*">
      <option name="FOREGROUND" value="bf002c" />
      <option name="BACKGROUND" value="ffc7d2" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ff002a" />
      <option name="ERROR_STRIPE_COLOR" value="e3dbe0" />
    </pattern>
  </component>
</application>